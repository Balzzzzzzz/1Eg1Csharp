Console.WriteLine("Zadanie 1");
Console.WriteLine("Dni miesiąca listopad 2022 r.: ");
for (int i = 1; i < 31; i++)
{
	if (i < 10)
	{
        Console.WriteLine( "0" + i + ".11.2022" );
    }
	else
	{
        Console.WriteLine( i + ".11.2022" );
    }
}
Console.ReadKey();



Console.WriteLine("Zadanie 2");
Console.Write("Wpisz ilość cyfr: ");
int n = int.Parse(Console.ReadLine());
for (int i = 1; i < n; i++)
{
	if (i % 2 != 0)
	{
        Console.WriteLine( Math.Pow(i , 2) );
    }
}
Console.ReadKey();


Console.WriteLine("Zadanie 3");
Console.WriteLine("Liczby 4-cyfrowe dzielące się przez 379: ");
for (int i = 1000; i < 10000; i++)
{
	if ( i % 379 == 0)
	{
		Console.Write(i + "; ");
	}
}
Console.ReadKey();


Console.WriteLine("Zadanie 4");
Console.WriteLine("Liczby 3-cyfrowe dzielące się przez 5,6 lub 11: ");
for (int i = 100; i < 1000; i++)
{
	if ( i % 5 == 0 && i % 6 == 0 || i % 11 == 0)
	{
		Console.Write(i + "; ");
	}
}
Console.ReadKey();


Console.WriteLine("Zadanie 5");
Console.Write("Wpisz ile liczb chcesz podac: ");
int n = int.Parse(Console.ReadLine());
int suma = 0;
for (int i = 0; i < n; i++)
{
    Console.Write("Wpisz liczbę: ");
    int a = int.Parse(Console.ReadLine());
    suma += a;
}
Console.Write("Suma tych liczb: " + suma);
Console.ReadKey();











